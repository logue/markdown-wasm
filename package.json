{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/markdown-wasm",
  "type": "module",
  "version": "1.4.10",
  "license": "MIT",
  "description": "Markdown parser and html generator implemented in WebAssembly",
  "homepage": "https://github.com/logue/markdown-wasm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/markdown-wasm.git"
  },
  "author": {
    "name": "rsms",
    "url": "https://rsms.me/"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "main": "dist/markdown.umd.js",
  "module": "dist/markdown.es.js",
  "types": "markdown.d.ts",
  "exports": {
    ".": {
      "import": "./dist/markdown.es.js",
      "require": "./dist/markdown.umd.js",
      "types": "./markdown.d.ts"
    }
  },
  "files": [
    "./src/markdown.wasm",
    "./src/markdown.js",
    "./src/markdown.d.ts",
    "./src/index.js",
    "./dist",
    "markdown.d.ts",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "build": "run-p build-wasm build-only",
    "build:clean": "rimraf dist",
    "build-wasm": "./src/build",
    "build-only": "vite build",
    "build-docs": "vite build --mode=docs",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "test": "vitest --root src",
    "test:spec": "node test/spec/spec.js",
    "test:example": "node example/example.js",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.7",
    "@types/node": "^20.4.9",
    "commonmark-spec": "^0.30.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.6.1",
    "vitest": "^0.34.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "semver": "^7.5.2",
    "yaml": "^2.2.2"
  }
}
