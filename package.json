{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/markdown-wasm",
  "version": "1.3.2",
  "type": "module",
  "description": "Markdown parser and html generator implemented in WebAssembly",
  "browser": "dist/markdown.js",
  "main": "dist/markdown.node.js",
  "jsnext:main": "dist/markdown.es.js",
  "module": "dist/markdown.es.js",
  "types": "markdown.d.ts",
  "homepage": "https://github.com/logue/markdown-wasm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/markdown-wasm.git"
  },
  "files": [
    "dist/markdown.wasm",
    "dist/markdown.js",
    "dist/markdown.js.map",
    "dist/markdown.node.js",
    "dist/markdown.node.js.map",
    "dist/markdown.es.js",
    "dist/markdown.es.js.map",
    "markdown.d.ts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/markdown.es.js",
      "default": "./dist/markdown.js",
      "type": "./markdown.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "src/build & vite build",
    "test": "node test/spec/spec.js",
    "lint": "eslint . && prettier . -w"
  },
  "author": {
    "name": "rsms",
    "url": "https://rsms.me/"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    }
  ],
  "license": "MIT",
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.3",
    "vite": "^4.0.4"
  }
}
